{
	"name": "nextjs-trpc-mongoose-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 4000",
		"build": "next build",
		"build:dev": "next build",
		"build:prod": "cross-env NODE_ENV=production next build",
		"build:stats": "cross-env ANALYZE=true npm run build",
		"export": "next export",
		"lint": "next lint && pnpm check-types",
		"start": "next start",
		"clean": "rimraf .next out",
		"prerelease": "npx semantic-release --no-ci && npm run pkg-version && git add . && git commit -m 'docs(changelog): update change log before releasing' && git push origin prerelease",
		"release": "npm run clean && npm run build && npm run export:prod && npm run copy-to-cli && npm run prerelease",
		"pkg-version": "npm pkg set version=$(echo $(git describe --tags $(git rev-list --tags --max-count=1)) | cut -c2-)",
		"format": "next lint --fix && prettier '**/*.{js,ts,tsx,json,yaml}' --write",
		"check-types": "tsc --noEmit --pretty",
		"commit": "git add . && cz --allow-empty && git push origin",
		"prepare": "husky install",
		"postbuild": "next-sitemap"
	},
	"dependencies": {
		"@ant-design/icons": "^5.0.1",
		"@next-auth/mongodb-adapter": "^1.1.3",
		"@next/env": "^13.2.4",
		"@sendgrid/mail": "^7.7.0",
		"@tanstack/react-query": "^4.20.2",
		"@trpc/client": "^10.9.0",
		"@trpc/next": "^10.9.0",
		"@trpc/react-query": "^10.9.0",
		"@trpc/server": "^10.9.0",
		"antd": "^5.3.2",
		"axios": "^1.3.4",
		"class-validator": "^0.14.0",
		"dayjs": "^1.11.7",
		"diginext-utils": "^2.1.26",
		"dotenv": "^16.0.3",
		"inquirer": "^8.2.5",
		"lodash": "^4.17.21",
		"mongodb": "^5.5.0",
		"mongoose": "^7.2.1",
		"next": "^13.2.1",
		"next-auth": "^4.19.0",
		"next-seo": "^5.15.0",
		"next-sitemap": "^4.0.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sanitize-html": "^2.10.0",
		"superjson": "1.9.1",
		"swagger-ui-react": "5.0.0-alpha.4",
		"trpc-openapi": "^1.1.2",
		"usehooks-ts": "^2.9.1",
		"uuid": "^9.0.0",
		"validator": "^13.9.0",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@commitlint/cz-commitlint": "^17.3.0",
		"@next/bundle-analyzer": "^13.0.6",
		"@semantic-release/changelog": "^6.0.2",
		"@semantic-release/git": "^10.0.1",
		"@tailwindcss/typography": "^0.5.9",
		"@types/eslint": "^8.21.1",
		"@types/lodash": "^4.14.191",
		"@types/node": "^18.14.0",
		"@types/prettier": "^2.7.2",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/sanitize-html": "^2.8.0",
		"@types/swagger-ui-react": "^4.11.0",
		"@types/validator": "^13.7.12",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"autoprefixer": "^10.4.13",
		"commitizen": "^4.2.5",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.14",
		"eslint": "^8.34.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-next": "^13.0.6",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.1.6",
		"eslint-plugin-jest-dom": "^4.0.3",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-tailwindcss": "^3.7.1",
		"eslint-plugin-testing-library": "^5.9.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.2",
		"lint-staged": "^13.0.4",
		"postcss": "^8.4.14",
		"prettier": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"rimraf": "^3.0.2",
		"semantic-release": "^19.0.5",
		"serve": "^14.1.2",
		"tailwindcss": "^3.2.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^4.9.5"
	},
	"ct3aMetadata": {
		"initVersion": "7.8.0"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"release": {
		"branches": [
			"prerelease",
			"dev/goon"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/digitopvn/nextjs-trpc-mongoose-starter.git"
	},
	"homepage": "https://github.com/digitopvn/nextjs-trpc-mongoose-starter",
	"author": "Goon Nguyen (https://github.com/mrgoonie)"
}
